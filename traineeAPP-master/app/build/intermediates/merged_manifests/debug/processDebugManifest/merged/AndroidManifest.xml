        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.thal3.myapplication.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.thal3.myapplication.LoginEstagiario"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.thal3.myapplication.LoginEmpregador"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.thal3.myapplication.Contato"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.thal3.myapplication.CadastroEstagiario"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.thal3.myapplication.dominio.Curriculo" />
        <activity android:name="com.example.thal3.myapplication.TelaPrincipalEstagiario" />
        <activity android:name="com.example.thal3.myapplication.Consulta" >
